<?xml version="1.0"?>

<PropertyList>

  <fx>
    <crank>
      <name>start</name>
      <mode>once</mode>
      <path>Sounds/coughing.wav</path>
      <condition>
        <property>engines/engine/running</property>
      </condition>
      <reference-dist>5.0</reference-dist>
      <max-dist>50.0</max-dist>
      <position>
        <x>0.5</x>
        <y>0</y>
        <z>0.2</z>
      </position>
      <volume>
        <property>sim/sound/cabin-volume</property>
      </volume>
    </crank>

    <crank>
      <name>crank</name>
      <mode>looped</mode>
      <path>Sounds/cranking.wav</path>
      <condition>
        <and>
          <property>engines/engine/cranking</property>
          <not>
            <property>engines/engine/running</property>
          </not>
        </and>
      </condition>
      <reference-dist>5.0</reference-dist>
      <max-dist>50.0</max-dist>
      <position>
        <x>0.5</x>
        <y>0</y>
        <z>0.2</z>
      </position>
      <volume>
        <property>sim/sound/cabin-volume</property>
      </volume>
    </crank>

    <rumble>
      <name>rumble</name>
      <mode>looped</mode>
      <path>Sounds/rumble.wav</path>
      <position>
        <x>0</x>
        <y>0</y>
        <z>-0.5</z>
      </position>
      <condition>
        <or>
          <property>gear/gear[0]/wow</property>
          <property>gear/gear[1]/wow</property>
          <property>gear/gear[2]/wow</property>
        </or>
      </condition>
      <reference-dist>1.0</reference-dist>
      <max-dist>5.0</max-dist>
      <volume>
        <property>velocities/groundspeed-kt</property>
        <factor>0.05</factor>
      </volume>
    </rumble>

    <squeal>
      <name>squeal</name>
      <path>Sounds/squeal.wav</path>
      <reference-dist>10.0</reference-dist>
      <max-dist>20</max-dist>
      <condition>
        <or>
          <property>gear/gear[0]/wow</property>
          <property>gear/gear[1]/wow</property>
          <property>gear/gear[2]/wow</property>
        </or>
      </condition>
      <position>
        <x>0</x>
        <y>0</y>
        <z>-0.5</z>
      </position>
      <volume>
        <property>velocities/airspeed-kt</property>
        <factor>0.01</factor>
      </volume>
      <volume>
        <internal>dt_stop</internal>
        <factor>0.05</factor>
        <max>1.0</max>
      </volume>
      <pitch>
        <property>velocities/airspeed-kt</property>
        <factor>0.0025</factor>
        <offset>1.2</offset>
      </pitch>
    </squeal>

    <flap>
      <name>flaps</name>
      <mode>in-transit</mode>
      <path>Sounds/flaps.wav</path>
      <property>surface-positions/flap-pos-norm</property>
      <position>
        <x>-0.3</x>
        <y>0</y>
        <z>-0.0</z>
      </position>
      <volume>
        <factor>0.45</factor>
      </volume>
      <reference-dist>2.0</reference-dist>
      <max-dist>3.0</max-dist>
    </flap>

  </fx>

</PropertyList>
