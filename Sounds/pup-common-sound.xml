<?xml version="1.0"?>

<PropertyList>

  <fx>

    <fuelpump>
      <name>fuelpump</name>
      <mode>looped</mode>
      <path>fuel-pump.wav</path>
      <condition>
        <greater-than>
          <property>systems/electrical/outputs/fuel-pump</property>
          <value>0</value>
        </greater-than>
      </condition>
      <reference-dist>5.0</reference-dist>
      <max-dist>10.0</max-dist>
      <position>
        <x>0.7</x>
        <y>0</y>
        <z>0.1</z>
      </position>
      <volume>
        <property>sim/sound/cabin-volume</property>
      </volume>
    </fuelpump>

    <crank>
      <name>start</name>
      <mode>once</mode>
      <path>Sounds/coughing.wav</path>
      <condition>
        <property>engines/engine/running</property>
      </condition>
      <reference-dist>5.0</reference-dist>
      <max-dist>50.0</max-dist>
      <position>
        <x>0.5</x>
        <y>0</y>
        <z>0.2</z>
      </position>
      <volume>
        <property>sim/sound/cabin-volume</property>
      </volume>
    </crank>

    <crank>
      <name>crank</name>
      <mode>looped</mode>
      <path>Sounds/cranking.wav</path>
      <condition>
        <and>
          <property>engines/engine/cranking</property>
          <not>
            <property>engines/engine/running</property>
          </not>
        </and>
      </condition>
      <reference-dist>5.0</reference-dist>
      <max-dist>50.0</max-dist>
      <position>
        <x>0.5</x>
        <y>0</y>
        <z>0.2</z>
      </position>
      <volume>
        <property>sim/sound/cabin-volume</property>
      </volume>
    </crank>

    <rumble>
      <name>rumble</name>
      <mode>looped</mode>
      <path>Sounds/rumble.wav</path>
      <position>
        <x>0</x>
        <y>0</y>
        <z>-0.5</z>
      </position>
      <condition>
        <or>
          <property>gear/gear[0]/wow</property>
          <property>gear/gear[1]/wow</property>
          <property>gear/gear[2]/wow</property>
        </or>
      </condition>
      <reference-dist>1.0</reference-dist>
      <max-dist>5.0</max-dist>
      <volume>
        <property>velocities/groundspeed-kt</property>
        <factor>0.05</factor>
      </volume>
    </rumble>

    <squeal>
      <name>squeal</name>
      <path>Sounds/squeal.wav</path>
      <reference-dist>10.0</reference-dist>
      <max-dist>20</max-dist>
      <condition>
        <or>
          <property>gear/gear[0]/wow</property>
          <property>gear/gear[1]/wow</property>
          <property>gear/gear[2]/wow</property>
        </or>
      </condition>
      <position>
        <x>0</x>
        <y>0</y>
        <z>-0.5</z>
      </position>
      <volume>
        <property>velocities/airspeed-kt</property>
        <factor>0.01</factor>
      </volume>
      <volume>
        <internal>dt_stop</internal>
        <factor>0.05</factor>
        <max>1.0</max>
      </volume>
      <pitch>
        <property>velocities/airspeed-kt</property>
        <factor>0.0025</factor>
        <offset>1.2</offset>
      </pitch>
    </squeal>

    <flap>
      <name>flaps</name>
      <mode>in-transit</mode>
      <path>Sounds/flaps.wav</path>
      <property>surface-positions/flap-pos-norm</property>
      <position>
        <x>-0.3</x>
        <y>0</y>
        <z>-0.0</z>
      </position>
      <volume>
        <factor>0.45</factor>
      </volume>
      <reference-dist>2.0</reference-dist>
      <max-dist>3.0</max-dist>
    </flap>

    <stall>
      <name>stall</name>
      <mode>looped</mode>
      <path>stall-horn.wav</path>
      <condition>
        <and>
          <property>sim/current-view/internal</property>
          <property>systems/electrical/outputs/stall-warning</property>
          <property>hazards/stall/stall-warning</property>
        </and>
      </condition>
      <reference-dist>5.0</reference-dist>
      <max-dist>10.0</max-dist>
      <position>
        <x>0.8</x>
        <y>1.0</y>
        <z>0.6</z>
      </position>
      <volume>
        <property>sim/sound/cabin-volume</property>
      </volume>
    </stall>

    <click>
      <name>click</name>
      <mode>once</mode>
      <path>Sounds/click.wav</path>
      <condition>
        <and>
          <property>sim/sound/click</property>
          <property>sim/current-view/internal</property>
        </and>
      </condition>
      <reference-dist>5.0</reference-dist>
      <max-dist>10.0</max-dist>
      <position>
        <x>0.7</x>
        <y>0.0</y>
        <z>0.1</z>
      </position>
      <volume>
        <property>sim/sound/cabin-volume</property>
      </volume>
    </click>

    <rainwindshield>
      <name>rainwindshield</name>
      <mode>looped</mode>
      <path>Sounds/rain-windshield.wav</path>
      <condition>
        <and>
          <greater-than>
            <property>environment/rain-norm</property>
            <value>0</value>
          </greater-than>
          <property>sim/current-view/internal</property>
        </and>
      </condition>
      <reference-dist>1.0</reference-dist>
      <max-dist>5.0</max-dist>
      <position>
        <x>0.6</x>
        <y>0.0</y>
        <z>0.3</z>
      </position>
      <volume>
        <property>environment/rain-norm</property>
      </volume>
      <pitch>
        <property>environment/rain-norm</property>
        <factor>0.2</factor>
        <offset>1.0</offset>
      </pitch>
    </rainwindshield>

    <rainoutside>
      <name>rainoutside</name>
      <mode>looped</mode>
      <path>Sounds/rain.wav</path>
      <condition>
        <and>
          <not>
            <property>sim/current-view/internal</property>
          </not>
          <greater-than>
            <property>environment/rain-norm</property>
            <value>0</value>
          </greater-than>
          <less-than>
            <property>position/altitude-agl-ft</property>
            <value>50</value>
          </less-than>
        </and>
      </condition>
      <reference-dist>100.0</reference-dist>
      <max-dist>100.0</max-dist>
      <position>
        <x>0.0</x>
        <y>0.0</y>
        <z>0.0</z>
      </position>
      <volume>
        <property>environment/rain-norm</property>
      </volume>
      <pitch>
        <property>environment/rain-norm</property>
        <factor>0.2</factor>
        <offset>0.6</offset>
      </pitch>
    </rainoutside>

    <thunder0>
      <name>thunder0</name>
      <mode>once</mode>
      <path>thunder-clap.wav</path>
      <condition>
        <property>sim/sound/thunder[0]/active</property>
      </condition>
      <reference-dist>1000.0</reference-dist>
      <max-dist>1000.0</max-dist>
      <volume>
        <property>sim/sound/thunder[0]/volume</property>
      </volume>
    </thunder0>

    <thunder1>
      <name>thunder1</name>
      <mode>once</mode>
      <path>thunder-clap-midrange.wav</path>
      <condition>
        <property>sim/sound/thunder[1]/active</property>
      </condition>
      <reference-dist>1000.0</reference-dist>
      <max-dist>1000.0</max-dist>
      <volume>
        <property>sim/sound/thunder[1]/volume</property>
      </volume>
      <volume>1.0</volume>
    </thunder1>

    <thunder2>
      <name>thunder2</name>
      <mode>once</mode>
      <path>thunder-midrange.wav</path>
      <condition>
        <property>sim/sound/thunder[2]/active</property>
      </condition>
      <reference-dist>1000.0</reference-dist>
      <max-dist>1000.0</max-dist>
      <volume>
        <property>sim/sound/thunder[2]/volume</property>
      </volume>
      <volume>1.0</volume>
    </thunder2>

    <thunder3>
      <name>thunder3</name>
      <mode>once</mode>
      <path>thunder-roll.wav</path>
      <condition>
        <property>sim/sound/thunder[3]/active</property>
      </condition>
      <reference-dist>1000.0</reference-dist>
      <max-dist>1000.0</max-dist>
      <volume>
        <property>sim/sound/thunder[3]/volume</property>
      </volume>
    </thunder3>

  </fx>

</PropertyList>
