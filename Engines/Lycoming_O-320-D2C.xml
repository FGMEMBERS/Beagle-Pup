<piston_engine name="Lycoming O-320-D2C">

  <!-- Basic Paremeters -->
  <minmp unit="INHG">12.0</minmp>
  <maxmp unit="INHG">28.5</maxmp>
  <man-press-lag>1.0</man-press-lag>
  <starter-torque>75</starter-torque>
  <starter-rpm>800</starter-rpm>
  <idlerpm>400.0</idlerpm>
  <maxrpm>2750.0</maxrpm>
  <maxhp>160.00</maxhp>
  <displacement unit="IN3">319.8</displacement>
  <cycles>4.0</cycles>
  <bore unit="IN">5.125</bore>
  <stroke unit="IN">3.875</stroke>
  <compression-ratio>7.0</compression-ratio>
  <sparkfaildrop>0.1</sparkfaildrop>
  <static-friction>1.5</static-friction>

  <!-- Advanced Parameters -->
  <bsfc>0.41</bsfc>
  <volumetric-efficiency>0.89</volumetric-efficiency>
  <air-intake-impedance-factor>0.05</air-intake-impedance-factor>
  <ram-air-factor>1.5</ram-air-factor>

  <!-- Cooling Control -->
  <cylinders>4.0</cylinders>
  <cylinder-head-mass unit="LBS">12</cylinder-head-mass>
  <cooling-factor>0.6</cooling-factor>

</piston_engine>
