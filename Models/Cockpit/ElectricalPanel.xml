    <property>controls/switches/land-taxi</property>
    <factor>-30</factor>
    <offset-deg>30</offset-deg>
    <center>
      <x-m>0.0037</x-m>
      <y-m>0.1573</y-m>
      <z-m>0.0040</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>controls/switches/land-taxi</property>
        <factor>1</factor>
        <min>0</min>
        <max>2</max>
