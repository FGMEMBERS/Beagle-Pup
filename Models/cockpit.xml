<PropertyList>

  <params>
    <panel-norm>sim/multiplay/generic/float[0]</panel-norm>
    <panel-light-volts>sim/multiplay/generic/float[3]</panel-light-volts>
  </params>

  <path>cockpit.ac</path>

  <animation>
    <type>material</type>
    <object-name>Cockpit</object-name>
    <condition>
      <greater-than>
        <property alias="../../../../params/panel-light-volts"/>
        <value>0.0</value>
      </greater-than>
    </condition>
    <emission>
      <red>0.20</red>
      <green>0.18</green>
      <blue>0.18</blue>
      <factor-prop alias="../../../params/panel-norm"/>
    </emission>
  </animation>

  <model>
    <path>Instruments/Fuel/FuelGauge24.xml</path>
    <name>FuelGauge24</name>
    <offsets>
      <x-m>1.6900</x-m>
      <y-m>-0.2500</y-m>
      <z-m> 0.2300</z-m>
    </offsets>
  </model>

  <animation>
    <type>select</type>
    <object-name>FuelGauge24</object-name>
    <condition>
      <less-than>
        <property>consumables/fuel/tank[1]/capacity-gal_imp</property>
        <value>15</value>
      </less-than>
    </condition>
  </animation>

  <model>
    <path>Instruments/Fuel/FuelGauge36.xml</path>
