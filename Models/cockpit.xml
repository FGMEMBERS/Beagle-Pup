<PropertyList>

  <path>cockpit.ac</path>

  <animation>
    <type>material</type>
    <object-name>Cockpit</object-name>
    <condition>
      <greater-than>
        <property>systems/electrical/outputs/panel-lights</property>
        <value>0.0</value>
      </greater-than>
    </condition>
    <emission>
      <red>0.20</red>
      <green>0.18</green>
      <blue>0.18</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>

  <model>
    <path>Instruments/Clock/Clock.xml</path>
    <offsets>
      <x-m>1.6900</x-m>
      <y-m>-0.1500</y-m>
