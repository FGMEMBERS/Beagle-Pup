<?xml version="1.0"?>

<PropertyList>

  <path>egt.ac</path>

  <animation>
    <type>material</type>
    <object-name>Face</object-name>
    <object-name>Needle</object-name>
    <condition>
      <property>systems/electrical/outputs/panel-lights</property>
    </condition>
    <emission>
      <red>0.7</red>
      <green>0.7</green>
      <blue>0.7</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Needle</object-name>
    <property>engines/engine/egt-degf</property>
    <interpolation>
      <entry>
        <ind>1100</ind>
        <dep>-45</dep>
      </entry>
      <entry>
        <ind>1600</ind>
        <dep>45</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.012</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>

  </animation>

</PropertyList>


